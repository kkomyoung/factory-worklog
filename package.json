{
  "name": "factory-worklog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:local": "next dev --turbopack",
    "dev:dev": "dotenv -e .env.development -- next dev --turbopack",
    "dev:prod": "dotenv -e .env.production -- next dev --turbopack",
    "swagger:generate": "node scripts/generate-swagger.js",
    "prebuild": "npm run swagger:generate",
    "build": "prisma generate && next build --turbopack",
    "build:local": "dotenv -e .env.local -- prisma generate && dotenv -e .env.local -- next build --turbopack",
    "build:dev": "dotenv -e .env.development -- prisma generate && dotenv -e .env.development -- next build --turbopack",
    "build:prod": "dotenv -e .env.production -- prisma generate && dotenv -e .env.production -- next build --turbopack",
    "start": "next start",
    "postinstall": "prisma generate",
    "db": "dotenv -e .env.local -- prisma studio",
    "db:dev": "dotenv -e .env.development -- prisma studio",
    "db:prod": "dotenv -e .env.production -- prisma studio",
    "db:sync": "dotenv -e .env.local -- prisma db push && prisma generate",
    "db:sync:dev": "dotenv -e .env.development -- prisma migrate deploy && prisma generate",
    "db:sync:prod": "dotenv -e .env.production -- prisma migrate deploy && prisma generate",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:seed:dev": "dotenv -e .env.development -- prisma db seed",
    "db:seed:prod": "dotenv -e .env.production -- prisma db seed",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "prisma": {
    "seed": "npx tsx prisma/seed/index.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.57.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.28.0",
    "tailwind-merge": "^3.3.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
